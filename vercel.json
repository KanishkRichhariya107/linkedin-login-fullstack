{
  "version": 2,
  "buildCommand": "cd api && npm install && cd ../Frontend && npm install && npm run build",
  "outputDirectory": "Frontend/dist",
  "functions": {
    "api/*.js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.js"
    }
  ]
}